{
	"name": "hehepx/hehep-hvalidation",
	"description": "hehep-hvalidation 是一个PHP验证器基础组件",
	"license": "Apache-2.0",
	"authors": [
		{
			"name": "hehe"
		}
	],
	"require": {
		"php": ">=7.4"
	},
	"require-dev": {
		"phpunit/phpunit": "^8.5"
	},
	"autoload": {
		"psr-4": {
			"hehe\\core\\hvalidation\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"hvalidation\\tests\\": "tests/"
		}
	},
	"scripts": {
		"test:win": ["@test:php7","@test:php8"],
		"test:php7": "D:/bin/phpstudy_pro/Extensions/php/php7.4.3nts/php.exe ../vendor/bin/phpunit",
		"test:php8": "D:/bin/phpstudy_pro/Extensions/php/php8.2.9nts/php.exe ../vendor/bin/phpunit"
	}

}
